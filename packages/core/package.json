    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "14.17.29",
    "@types/uuid": "^8.3.1",
    "graphql": "^16.0.1",
    "zod": "^3.11.6"
  },
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "zod": "^3.0.0"
